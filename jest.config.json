{
  "transform": {
    ".(t|j)sx?$": "ts-jest"
  },
  "testEnvironment": "jsdom",
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/"
  ]
}
